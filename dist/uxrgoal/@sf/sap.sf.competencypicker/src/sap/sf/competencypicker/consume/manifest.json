{
	"_version": "1.8.0",
	"sap.app": {
		"id": "sap.sf.consume",
		"type": "application",
		"i18n": "i18n/messagebundle.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "Consume competency picker",
		"description": "Sample component to consume competency picker",
